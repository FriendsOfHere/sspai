{
   "name": "少数派",
   "version": "1.4.0",
   "description": "展示少数派最新文章",
   "identifier": "foh.sspai",
   "createdBy": "Lifesign",
   "icon": "icon.png",
   "website": "https://github.com/FriendsOfHere/sspai",
   "appcast": "https://raw.githubusercontent.com/FriendsOfHere/sspai/master/appcast.xml",
   "script": "index.js",
   "defaultCategory": "News",
   "queryKeywords": ["sspai", "pai"],
   "displayOn": [
      "mini",
      "menuBar",
      "dock"
   ],
   "preferences": [
      {
          "title": "更新时间(h)",
          "type": "text",
          "key": "update-frequency",
          "default": "2"
      },
      {
          "title": "文章拉取数量",
          "type": "popup",
          "key": "article-num",
          "default": 0,
          "options": [
              "10",
              "15",
              "20"
          ]
      },
      {
          "title": "未读消息提醒",
          "type": "popup",
          "key": "unread-notify",
          "default": 0,
          "options": [
              "关闭",
              "开启",
          ]
      },
      {
          "title": "菜单栏图标样式",
          "type": "popup",
          "key": "menuBar-icon-style",
          "default": 0,
          "options": [
              "默认样式",
              "样式二",
              "样式三",
              "样式四",
          ]
      },
      {
          "title": "DEBUG 快捷键",
          "type": "text",
          "key": "debug-hotkey",
          "default": "cmd+F1",
      }
   ]
}
